<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name </th>
      <th>Email </th>
      <th>Role Id </th>
      <th>image</th>
      <th>City</th>
      <th>Postcode</th>
      <th>Start date</th>
      <th>Active</th>
      <th>Company Id</th>
      <th>End Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>




  <tbody>
    <% @user.each do |user| %>
      <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.email %></td>
      <td><%= user.role_id %></td>
     <td><%= user.image  %></td>
     <td><%= user.city.city_name %></td>
     <td><%= user.postcode %></td>
     <td><%= user.start_date %></td>
     <td><%= user.active %></td>
     <td><%= user.company_id %></td>
     <td><%= user.end_date %></td>


       
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', root_path %>


 
